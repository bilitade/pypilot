# =================================
# PyVibe Project .gitignore
# =================================

# Root level
.env
*.log
.DS_Store
Thumbs.db

# =================================
# Backend (Python)
# =================================

# Byte-compiled / optimized files
backend/__pycache__/
backend/*.py[cod]
backend/*$py.class

# Distribution / packaging
backend/build/
backend/dist/
backend/*.egg-info/

# Virtual environment
backend/.venv

# Python cache
backend/.coverage
backend/.coverage.*
backend/.cache
backend/.pytest_cache/
backend/.mypy_cache/
backend/htmlcov/

# LangGraph
backend/.langgraph_api

# =================================
# Frontend (VS Code Extension)
# =================================

# Node.js
pyvibe-extension/node_modules/

# Build outputs
pyvibe-extension/dist/
pyvibe-extension/out/
pyvibe-extension/*.vsix

# VS Code testing
pyvibe-extension/.vscode-test/

# =================================
# IDE files
# =================================

# VS Code (keep essential configs)
.vscode/
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json

# =================================
# OS files
# =================================

# macOS
.DS_Store

# Windows
Thumbs.db
